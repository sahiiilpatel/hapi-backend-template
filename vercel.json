{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "server/**/*",
                    "config/**/*"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/server.js"
        }
    ]
}